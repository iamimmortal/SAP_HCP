{
    "exposed": true,
    "authentication": [
    {"method" : "Basic"}],
    "force_ssl": false,
    "enable_etags": true,
    "prevent_xsrf": false,
    "anonymous_connection": null,
    "cors": [{
        "enabled": true,"allowMethods": [
   "GET",
   "POST",
   "HEAD",
   "OPTIONS"
   ],
   "allowOrigin": ["*"],
   "maxAge": "3600"
    }],
    
    "cache_control": "no-cache, no-store",
    "default_file": "index.html",
    "allowHeaders": [
		"Accept",
		"Authorization",
		"Content-Type",
		"X-CSRF-Token",
		"Access-Control-Allow-Origin"
	],
	"exposeHeaders": [
		"x-csrf-token"
]
}